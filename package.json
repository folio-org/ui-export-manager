{
  "name": "@folio/export-manager",
  "version": "2.3.1",
  "description": "Description for export manager",
  "main": "index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "stripes": {
    "actsAs": [
      "app"
    ],
    "displayName": "ui-export-manager.meta.title",
    "route": "/export-manager",
    "okapiInterfaces": {
      "configuration": "2.0",
      "data-export-spring": "1.0",
      "organizations.organizations": "1.0",
      "tags": "1.0",
      "users": "15.0 16.0"
    },
    "icons": [
      {
        "name": "app",
        "alt": "View export manager",
        "title": "Export manager"
      }
    ],
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "module.export-manager.enabled",
        "displayName": "UI: Export manager module is enabled",
        "visible": false
      },
      {
        "permissionName": "ui-export-manager.jobs.downloadAndResend",
        "displayName": "Export manager: Download and re-send files",
        "description": "",
        "visible": true,
        "subPermissions": [
          "data-export.job.item.download",
          "data-export.job.item.resend"
        ]
      },
      {
        "permissionName": "ui-export-manager.export-manager.all",
        "displayName": "Export manager: All",
        "description": "",
        "visible": true,
        "subPermissions": [
          "module.export-manager.enabled",
          "configuration.entries.collection.get",
          "organizations.organizations.collection.get",
          "organizations.organizations.item.get",
          "data-export.config.collection.get",
          "data-export.config.item.get",
          "data-export.job.collection.get",
          "data-export.job.item.get",
          "data-export.edifact.orders.create",
          "tags.collection.get",
          "usergroups.collection.get",
          "users.collection.get",
          "users.item.get",
          "ui-export-manager.jobs.downloadAndResend"
        ]
      }
    ]
  },
  "scripts": {
    "build": "yarn stripes build --output ./output",
    "lint": "eslint .",
    "start": "yarn stripes serve",
    "test:unit": "jest --ci --coverage && yarn run test:unit:report",
    "test:unit:report": "cp -r ./artifacts/coverage-jest ./artifacts/coverage",
    "test:e2e": "echo Not implemented",
    "test": "yarn run test:unit && yarn run test:e2e",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-export-manager ./translations/ui-export-manager/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.7.4",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^8.0.0",
    "@folio/stripes-cli": "^2.0.0",
    "@folio/stripes-testing": "^4.4.0",
    "@formatjs/cli": "^4.2.16",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.1",
    "babel-jest": "^26.3.0",
    "core-js": "^3.6.1",
    "eslint": "^6.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^24.0.0",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-query": "^3.6.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "~4.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.5.10",
    "query-string": "^6.1.0",
    "react-router-prop-types": "^1.0.4"
  },
  "peerDependencies": {
    "@folio/stripes": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0 ",
    "react-query": "^3.6.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
