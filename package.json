{
  "name": "@folio/export-manager",
  "version": "5.0.0",
  "description": "Description for export manager",
  "main": "index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-export-manager.meta.title",
    "route": "/export-manager",
    "okapiInterfaces": {
      "configuration": "2.0",
      "data-export-spring": "2.0",
      "tags": "1.0",
      "users": "15.0 16.0"
    },
    "optionalOkapiInterfaces": {
      "organizations.organizations": "1.0"
    },
    "icons": [
      {
        "name": "app",
        "alt": "View export manager",
        "title": "Export manager"
      }
    ],
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "module.export-manager.enabled",
        "displayName": "UI: Export manager module is enabled",
        "subPermissions": [
          "stripes-core.settings.read"
        ],
        "visible": false
      },
      {
        "permissionName": "settings.export-manager.enabled",
        "displayName": "Settings (Export manager): Export manager settings enabled",
        "subPermissions": [
          "settings.enabled",
          "stripes-core.settings.read"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-export-manager.export-manager.view",
        "displayName": "Export manager: View",
        "description": "",
        "visible": true,
        "subPermissions": [
          "module.export-manager.enabled",
          "configuration.entries.collection.get",
          "organizations.organizations.collection.get",
          "organizations.organizations.item.get",
          "data-export.config.collection.get",
          "data-export.config.item.get",
          "data-export.job.collection.get",
          "data-export.job.item.get",
          "tags.collection.get",
          "usergroups.collection.get",
          "users.collection.get",
          "users.item.get"
        ]
      },
      {
        "permissionName": "ui-export-manager.jobs.downloadAndResend.execute",
        "displayName": "Export manager: Download and resend files",
        "description": "",
        "replaces": ["ui-export-manager.jobs.downloadAndResend"],
        "visible": true,
        "subPermissions": [
          "data-export.job.item.post",
          "data-export.job.item.download",
          "data-export.job.item.resend"
        ]
      },
      {
        "permissionName": "ui-export-manager.export-manager.all",
        "displayName": "Export manager: All",
        "description": "",
        "visible": true,
        "subPermissions": [
          "ui-export-manager.export-manager.view",
          "data-export.edifact.orders.create",
          "ui-export-manager.jobs.downloadAndResend.execute"
        ]
      },
      {
        "permissionName": "ui-export-manager.settings.view",
        "displayName": "Settings (Export manager): Can view only settings",
        "subPermissions": [
          "settings.export-manager.enabled",
          "data-export.job-deletion-intervals.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-export-manager.settings.edit",
        "displayName": "Settings (Export manager): Can view and edit settings",
        "subPermissions": [
          "ui-export-manager.settings.view",
          "data-export.job-deletion-intervals.item.put"
        ],
        "visible": true
      }
    ]
  },
  "scripts": {
    "build": "yarn stripes build --output ./output",
    "lint": "eslint .",
    "start": "yarn stripes serve",
    "test:unit": "jest --ci --coverage && yarn run test:unit:report",
    "test:unit:report": "cp -r ./artifacts/coverage-jest ./artifacts/coverage",
    "test:e2e": "echo Not implemented",
    "test": "yarn run test:unit && yarn run test:e2e",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "stripes translate compile"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.7.4",
    "@folio/eslint-config-stripes": "^8.0.0",
    "@folio/jest-config-stripes": "^3.0.0",
    "@folio/stripes": "^10.0.0",
    "@folio/stripes-cli": "^4.0.0",
    "@folio/stripes-testing": "^5.0.0",
    "@formatjs/cli": "^6.6.0",
    "core-js": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^24.0.0",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-query": "^3.6.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "^7.0.0",
    "final-form": "^4.0.0",
    "final-form-arrays": "^3.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.5.10",
    "query-string": "^6.1.0",
    "react-final-form": "^6.0.0",
    "react-final-form-arrays": "^3.0.0",
    "react-router-prop-types": "^1.0.4"
  },
  "peerDependencies": {
    "@folio/stripes": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-query": "^3.6.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
